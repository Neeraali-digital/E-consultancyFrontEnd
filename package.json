{
  "name": "wayzon-consultancy-frontend",
  "version": "1.0.1",
  "description": "Wayzon Educational Consultancy - Premium Frontend Application",
  "author": "Wayzon Educational Consultancy",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve --open",
    "build": "ng build",
    "build:prod": "node cache-bust.js && ng build --configuration production",
    "build:staging": "node cache-bust.js && ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "serve:ssr:E-consultancyFrontend": "node dist/E-consultancyFrontend/server/server.mjs",
    "vercel-build": "node cache-bust.js && node vercel-build.js",
    "deploy": "npm run build:prod && npm run vercel-build",
    "deploy:prod": "npm run build:prod && vercel --prod",
    "deploy:staging": "npm run build:staging && vercel",
    "preview": "ng serve --configuration production --port 4200",
    "clean": "rm -rf dist node_modules/.cache .angular/cache",
    "reinstall": "npm run clean && npm install",
    "favicon": "node generate-favicons.js"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.2",
    "@angular/cdk": "^20.1.2",
    "@angular/common": "^20.1.2",
    "@angular/compiler": "^20.1.2",
    "@angular/core": "^20.1.2",
    "@angular/forms": "^20.1.2",
    "@angular/material": "^20.1.2",
    "@angular/platform-browser": "^20.1.2",
    "@angular/platform-server": "^20.1.2",
    "@angular/router": "^20.1.2",
    "@angular/ssr": "^20.1.2",
    "chart.js": "^4.5.0",
    "express": "^5.1.0",
    "ng2-charts": "^8.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.2",
    "@angular/cli": "^20.1.2",
    "@angular/compiler-cli": "^20.1.2",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "autoprefixer": "^10.4.21",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2"
  }
}
